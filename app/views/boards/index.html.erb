<h1>Boards</h1>
<div class="container">
  <h2>New Board</h2>
  <%= form_for @board do |f| %>
    <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.submit %>
<% end %>
</div>

<br>

<div class="container">
  <% @boards.each do |board| %>
    <%= link_to board.title, board_path(board) %><br>
<% end %>
</div>

